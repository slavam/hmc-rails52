-# =@other_observation.inspect
.row
  %div{ class: "col-md-6 col-md-offset-3"}
    = form_with model: @other_observation, url: "create_precipitation?period="+@other_observation.period+"&obs_date="+@other_observation.obs_date.strftime("%Y-%m-%d"), method: :post do |f| 
      = f.label "Значение"
      = f.number_field :value, step: :any
      
      = f.label "Описание"
      = f.text_field :description
      
      = f.submit "Сохранить", class: "btn btn-primary"